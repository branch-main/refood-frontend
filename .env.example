# ReeFood Frontend Environment Variables

# Backend API URLs
VITE_API_BASE_URL=http://localhost:8080/api/v1        # Django backend
VITE_JAVA_API_BASE_URL=http://localhost:8081/api/v1   # Java/Spring backend (orders, payments)

# App Configuration
VITE_APP_NAME=ReeFood
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_GEOLOCATION=true

# Default Map Center (optional)
VITE_DEFAULT_LAT=40.712776
VITE_DEFAULT_LNG=-74.005974

# Pagination (optional)
VITE_DEFAULT_PAGE_SIZE=20
